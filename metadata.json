{
  "name": "otherskins-ansible",
  "version": "2.0.2",
  "author": "otherskins",
  "summary": "This module install and configure Ansible.",
  "license": "Apache-2.0",
  "source": "https://github.com/otherskins/puppet-ansible.git",
  "project_page": "https://github.com/otherskins/puppet-ansible",
